{
	"$schema": "https://vega.github.io/schema/vega-lite/v5.json",
	"width": 800,
	"height": 500,
	"title": "Earthquakes of magnitude 4 or large between 4 Oct 2020 and 10 Oct 2020",
	"projection": { "type": "mercator" },
	"layer": [
		{
			"data": {
				"url": "https://raw.githubusercontent.com/divideofficial/FIT3179-assignment-2/master/data/world_map.topojson",
				"format": { "type": "topojson", "feature": "world_map_geo" }
			},
			"mark": { "type": "geoshape", "fill": "lightgray", "stroke": "white" }
		},
		{
			"data": {
				"url": "https://raw.githubusercontent.com/KaneSec/vega_lite/main/2_symbol_map/data/earthquake.csv"
			},
			"mark": "geoshape",
			"encoding": {
				"color": {
					"field": "2019 Mortality Rate",
					"type": "quantitative"
				}
			}
		}
	]
}
